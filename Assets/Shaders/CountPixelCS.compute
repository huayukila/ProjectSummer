#pragma kernel CSMain

RWBuffer<int> CountBuffer;
RWTexture2D<float4> Result;
float4 TargetColorA;
float4 TargetColorB;

[numthreads(8, 8, 1)]
void CSMain (uint3 id : SV_DispatchThreadID)
{
    int tmp=0;
    if(Result[id.xy].x==1){
        tmp=1;
    }
    InterlockedAdd(CountBuffer[0],tmp);
    tmp=0;
    if(Result[id.xy].y==1){
        tmp=1;
    }
    InterlockedAdd(CountBuffer[1],tmp);
}